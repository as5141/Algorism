package back_array;

import java.util.Scanner;

public class array_3 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int count = 9;
		int[] arr = new int[count];
		String[] arrS = null;
		int[] check = new int[count];
		int A = scan.nextInt();
		int B = scan.nextInt();
		int C = scan.nextInt();
		String sum = Integer.toString(A*B*C);
		System.out.println(sum);
		arrS = sum.split("");
		
		for(int s=0;s<count;s++) {
			arr[s] += 1;
			check[s] = 0;
			System.out.println("arr[s]"+arr[s]);
			System.out.println("check[s]"+check[s]);
		}
		for(int i=0;i<count;i++) {
			for(int j=0; j<count;j++) {
				if(arr[i] == Integer.parseInt(arrS[i])) {
					check[i] += 1;
					System.out.println("여기 들가나"+check[i]);
				}
			}
		}
		for(int i : check) {
			System.out.println(i);
		}
	}
}
