package back_while;

import java.util.Scanner;

public class while_3 {
	public static void main(String[] args) {
		Scanner scan  = new Scanner(System.in);
		String first = scan.nextLine();
		String[] origin = new String[2];
		int[] intS = new int[2];
		String[] Str = new String[2];
		String[] Str2 = new String[2];
		int count = 0;
		String sums="";
		String tmp="";
		int sum = 0;
		
		if(first != "x") {
			if(Integer.parseInt(first) <= 10) {
				first="0"+first;
			}
			Str = SArray(first);
			origin = Str;
			for(int i=0; i<=1; i++) {
				intS[i] = Integer.parseInt(Str[i]);
				sum += intS[i];
				System.out.println("origin"+i+" : "+origin[i]);
				System.out.println("intS"+i+" : " + intS[i]);
				System.out.println("sum : "+sum);
			}
			first = "x";
			count++;
		}
		while(true) {
			System.out.println("들감?");
			//10이 안되면 앞에 0 붙여줌
			if(sum < 10) {
				sums = "0"+sum;
			}
			else {
				sums = Integer.toString(sum);
			}
			Str2 = SArray(sums);
			sum = Integer.parseInt(Str[1]) + Integer.parseInt(Str2[1]);
			tmp = Str[1];
			System.out.println("tmp : "+tmp);
			System.out.println("Str[1] : "+Str[1]);
			System.out.println("Str2[1] : "+Str2[1]);
			System.out.println("sum : "+sum);
			Str[1] = Str2[1];
			if(origin[0].equals(tmp)) {
				if(origin[1].equals(Str1[1])) {
					break;
				}
			}
			
			count++;
			System.out.println("tmp : "+tmp);
			System.out.println("Str[1] : "+Str[1]);
			System.out.println("Str2[1] : "+Str2[1]);
			System.out.println("sum : "+sum);
			
		}
		System.out.println(count);

	}
	static String[] SArray(String sums) {
		String[] temp = new String[2];
		temp[0] = sums.substring(0,1);
		temp[1] = sums.substring(1,2);
		return temp;
	}
}
